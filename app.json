{
  "expo": {
    "name": "Nexus Dev",
    "slug": "Nexus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.meister.Nexus.dev",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.Nexus"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      "@logrocket/react-native",
      "expo-secure-store",
      "expo-font",
      [
        "expo-image-picker",
        {
          "cameraPermission": "Diese App benötigt Zugriff auf deine Kamera.",
          "photosPermission": "Diese App benötigt Zugriff auf deine Fotos."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2b67df09-b008-4ffa-ae7d-5de04984cb89"
      }
    }
  }
}
